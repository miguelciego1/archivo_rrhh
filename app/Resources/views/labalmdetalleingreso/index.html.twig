                <td>
                    <ul>
                        <li>
                            <a href="{{ path('labalmdetalleingreso_edit', { 'id': labAlmDetalleingreso.id }) }}">Editar</a>
                        </li>
                        <li>
                            <a href="{{ path('labalmdetalleingreso_delete', { 'id': labAlmDetalleingreso.id }) }}">Eliminar</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    </div>
</div>
</div>
</div>
</div>
