{% extends 'main.html.twig' %}

{% block body %}

    <div class="row">
              <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
              {% for flashMessaje in app.session.flashbag.get ('mensaje')%}
            <div  class="alert alert-success alert-dismissible" role="alert" >
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                 {{ flashMessaje}}
            </div>
        {% endfor%}
        {% for flashMessaje in app.session.flashbag.get ('advertencia')%}
            <div  class="alert alert-warning alert-dismissible" role="alert" >
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                 {{ flashMessaje}}
            </div>
        {% endfor%}
        {% for flashMessaje in app.session.flashbag.get ('error')%}
            <div  class="alert alert-danger alert-dismissible" role="alert" >
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                 {{ flashMessaje}}
            </div>
        {% endfor%}
                <div class="x_panel">
                  <div class="x_title">
                    <h2>LISTADO <small>Registro de Egresos</small></h2>
                    <!-- ----------------------------------------------- -->

                    <div class="title_right">
                         <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                         <a  class="btn btn-success" href="{{ path('labalmegreso_new') }}" >
                          <i class="glyphicon glyphicon-plus-sign"> NUEVO EGRESO DE PRODUCTOS</i>
                        </a>
                         </div>
                       </div>
                      <!-- ------------------------------------ -->

                    <div class="clearfix"></div>
                  </div>

                  <div class="x_content">
                    <div class="table-responsive">
    <table class="table">
        <thead>
            <tr class="headings">
                <th class="column-title">NRO</th>
                <th class="column-title">RECIBIDO POR</th>
                <th class="column-title">SECCION</th>
                <th class="column-title">ESTADO</th>
                <th class="column-title">OPCIONES</th>
            </tr>
        </thead>
        <tbody>
        {% for labAlmEgreso in labAlmEgresos %}
            <tr>
                <td>{{ labAlmEgreso.id }}</td>
                <td>{{ labAlmEgreso.recibidoPor}}</td>
                <td>{% if labAlmEgreso.seccion=='R' %}
                <span>RC-IVA</span>

                {% elseif labAlmEgreso.seccion=='P'%}
                <span>PLANILLA</span>

                {% elseif labAlmEgreso.seccion=='A'%}
                <span>ARCHIVO</span>

                {% elseif labAlmEgreso.seccion== 'E'%}
                <span>EVALUACION Y CAPACITACION</span>

                {% elseif labAlmEgreso.seccion=='AFP'%}
                <span>AFP</span>

                {% elseif labAlmEgreso.seccion== 'CA'%}
                <span>CONTROL ASISTENCIA</span>

                {% elseif labAlmEgreso.seccion== 'SU'%}
                <span>SUBSIDIO</span>

                {% elseif labAlmEgreso.seccion== 'S'%}
                <span>SECRETARIA</span>

                {% elseif labAlmEgreso.seccion== 'AL'%}
                <span>ASESORIA LEGAL</span>
                {% endif %}</td>
                <td>{% if labAlmEgreso.estado ==1%} <span>En Proceso</span>{% elseif labAlmEgreso.estado == 2 %}<span>Confirmado</span>{% endif %}</td>
                
                <td>
                            <a href="{{ path('labalmegreso_show', { 'id': labAlmEgreso.id }) }}" class="btn btn-info"><i class="fa fa-eye" aria-hidden="true"></i> Mostrar</a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    </div>
</div>
</div>
</div>
</div>
{% endblock %}
